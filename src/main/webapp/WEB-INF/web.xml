<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>LegalWhen</display-name>
    <servlet>
        <servlet-name>lawordate</servlet-name>
        <servlet-class>oeg.legalwhen.lawordate.Lawordate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getText</servlet-name>
        <servlet-class>oeg.legalwhen.lawordate.TestLawORDate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lawordate</servlet-name>
        <url-pattern>/lawordate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestLawORDate</servlet-name>
        <url-pattern>/TestLawORDate</url-pattern>
    </servlet-mapping>
</web-app>
